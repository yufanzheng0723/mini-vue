{
  "compilerOptions": {
    "outDir": "dist", // 打包后的目录
    "sourceMap": false, // 是否生成sourceMap
    "target": "es2016",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": ["ESNext", "DOM"],
    "baseUrl": ".",
    "paths": {
      "@mini-vue/*": ["packages/*/src"]
    }
  }
}
